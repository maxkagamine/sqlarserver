{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore.Mvc": "Warning",
      "Microsoft.AspNetCore.Routing": "Warning",
      "Microsoft.AspNetCore.DataProtection": "None"
    },
    "Console": {
      "FormatterOptions": {
        "SingleLine": true
      }
    }
  },
  "SizeFormat": "Binary", // Bytes, Binary, SI
  "DirectoriesFirst": true,
  "CaseInsensitive": false,
  "StaticSite": false,
  "Charset": "utf-8"
}
